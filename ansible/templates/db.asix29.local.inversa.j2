;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     asix29.local. root.asix29.local. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.asix29.local.
3       IN      PTR     ns.asix29.local.
3       IN      PTR     svr.asix29.local.
1       IN      PTR     rt.asix29.local.
100     IN      PTR     cli.asix29.local.
